python
Copy code
import requests
from bs4 import BeautifulSoup
import sqlite3

# SQLite bazasini yaratish
conn = sqlite3.connect('namoz_vaqtlari.db')
cursor = conn.cursor()

# Jadvalni o'chirish (agar mavjud bo'lsa)
cursor.execute('DROP TABLE IF EXISTS namoz_vaqtlari')

# Jadvalni yaratish
cursor.execute('''
CREATE TABLE IF NOT EXISTS namoz_vaqtlari (
    id INTEGER PRIMARY KEY,
    viloyat TEXT,
    yil INTEGER,
    milodiy_month INTEGER,
    hijriy_day TEXT,
    weekday TEXT,
    sahar TEXT,
    quyosh TEXT,
    pesin TEXT,
    asr TEXT,
    iftor TEXT,
    hufton TEXT
)
''')

# Islomuz saytining URL manzili
