# Jadvalni yaratish
cursor.execute('''
CREATE TABLE IF NOT EXISTS namoz_vaqtlari (
    id INTEGER PRIMARY KEY,
    viloyat TEXT,         -- Viloyat ustuni
    yil INTEGER,         -- Yil ustuni (agar kerak bo'lsa)
    milodiy_month INTEGER, -- Milodiy oy (son tipida)
    hijriy_day TEXT,
    weekday TEXT,
    sahar TEXT,
    quyosh TEXT,
    pesin TEXT,
    asr TEXT,
    iftor TEXT,
    hufton TEXT
)
''')
